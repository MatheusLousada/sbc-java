package br.gov.sp.saobernardo.webservice.paradigma.dao;

/**
 * 
 */
public class AmbientesException extends RuntimeException {

	private static final long serialVersionUID = 1L;

	public AmbientesException() {
		super();
	}

	public AmbientesException(String message) {
		super(message);
	}

	public AmbientesException(Throwable cause) {
		super(cause);
	}

	public AmbientesException(String message, Throwable cause) {
		super(message, cause);
	}

}
